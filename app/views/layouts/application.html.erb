        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-item nav-link active"><%= link_to "Home", homes_path, class:"nav-item nav-link"  %></a>
          <div class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" id="navbarDropdownMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              SDEV120
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
              <a class="dropdown-item" href="#"><%= link_to "Individual Final", abouts_path, class:"nav-item nav-link"  %></a>
              <a class="dropdown-item" href="#"><%= link_to "Group Final", sdev120_gfs_path, class:"nav-item nav-link"  %></a>
            </div>
          </div>
          <div class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" id="navbarDropdownMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              SDEV140
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
              <a class="dropdown-item" href="#">Individual Final</a>
              <a class="dropdown-item" href="#">Python Scripts</a>
            </div>
          </div>
          <div class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" id="navbarDropdownMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              SDEV153
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
              <a class="dropdown-item" href="#">Individual Final</a>
              <a class="dropdown-item" href="#">Group Final</a>
            </div>
          </div>
          <a class="nav-item nav-link"><%= link_to 'About', abouts_path, class:"nav-item nav-link" %></a>
        </div>
      </div>
    </nav>
    <%= yield %>
